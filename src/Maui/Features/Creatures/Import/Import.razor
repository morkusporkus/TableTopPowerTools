@page "/import"
@using TabletopPowerTools.Core.Features.Creatures.Import;

<CascadingValue Value="_importState">
    @if (_importState.ImportStatus == ImportStatus.Upload)
    {
        <Upload />
    }
    else if(_importState.ImportStatus == ImportStatus.Review)
    {
        <Review />
    }
</CascadingValue>

